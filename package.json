{
  "name": "parcel_tracker_bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --quiet",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm_run": "npm run typeorm -- -d src/db/index.ts",
    "migration:generate": "npm run typeorm_run migration:generate ./app/src/db/migrations/$npm_config_name$_migration",
    "migration:create": "npm run typeorm -- migration:create ./src/db/migrations/$npm_config_name$_migration",
    "migration:run": "npm run typeorm_run migration:run"
  },
  "keywords": [],
  "author": "Maxim Gladkov",
  "license": "ISC",
  "description": "This bot tracking parcels",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "axios": "^1.12.1",
    "mysql": "^2.18.1",
    "node-cron": "^4.2.1",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
